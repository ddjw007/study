    "version": "0.2.0",
    "configurations": [
        {
            "name": "opencvv-gdb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/C++/opencv/ov_3rd/1/code/main",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/C++/opencv/ov_3rd/1",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb.exe",
            "setupCommands": [
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }

    ]
}